{
    "manifestVersion": 1,
    "id": "CollectionOfDemoTasks",
    "version": "0.0.1",
    "name": "Demo set of tasks",
    "publisher": "fgiele",
    "description": "Collection of tasks that demonstrate how to create custom tasks, build and test them",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "static/demo.png"
    },
    "scopes": [
        "vso.build"
    ],
    "categories": [
        "Azure Pipelines"
    ],
    "branding": {
        "color": "rgb(34, 34, 34)",
        "theme": "dark"
    },
    "content": {
        "details": {
            "path": "static/readme.md"
        }
    },
    "links": {
        "getstarted": {
            "uri": "https://github.com/fgiele"
        },
        "support": {
            "uri": "https://github.com/fgiele"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/fgiele"
    },
    "contributions": [
        {
            "id": "DemoResttask",
            "type": "ms.vss-distributed-task.task",
            "targets": [
              "ms.vss-distributed-task.tasks"
            ],
            "properties": {
              "name": "DemoResttask"
            }
        }
    ],
    "files": [
        {
            "path": "dist/src",
            "addressable": true
        },
        {
            "path": "static",
            "addressable": true
        }
    ]
}