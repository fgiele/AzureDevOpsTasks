{
    "manifestVersion": 1,
    "id": "CollectionOfDemoTasks",
    "version": "0.1.2",
    "name": "Demo set of tasks",
    "publisher": "FreekGiele",
    "description": "Collection of tasks that demonstrate how to create custom tasks, build and test them",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "static/demo-icon.png"
    },
    "scopes": [
        "vso.build"
    ],
    "categories": [
        "Azure Pipelines"
    ],
    "content": {
        "details": {
            "path": "static/readme.md"
        }
    },
    "links": {
        "getstarted": {
            "uri": "https://github.com/fgiele/AzureDevOpsTasks/README.md"
        },
        "support": {
            "uri": "https://github.com/fgiele/AzureDevOpsTasks"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/fgiele/AzureDevOpsTasks"
    },
    "contributions": [
        {
            "id": "restdemo-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "task/restdemo"
            }
        }
    ],
    "files": [
        {
            "path": "dist",
            "packagePath": "task/restdemo"
        },
        {
            "path": "src/restdemoV1/icon",
            "packagePath": "task/restdemo/restdemoV1"
        },
        {
            "path": "src/restdemoV2/icon",
            "packagePath": "task/restdemo/restdemoV2"
        },
        {
            "path": "static",
            "addressable": true
        },
        {
            "path": "src/restdemoV1/node_modules",
            "packagePath": "task/restdemo/restdemoV1/node_modules"
        },
        {
            "path": "src/restdemoV2/node_modules",
            "packagePath": "task/restdemo/restdemoV2/node_modules"
        }
    ]
}